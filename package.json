{
  "name": "size-satisfies",
  "version": "1.0.0",
  "description": "Same as SemVer.satisfies but for file size!",
  "main": "index.js",
  "scripts": {
    "test": "cross-env eslint index.js && jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fraxken/size-satisfies.git"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "size",
    "satisfies",
    "utility",
    "parse",
    "parser",
    "convert",
    "converter"
  ],
  "jest": {
    "collectCoverageFrom": [
      "index.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/"
    ]
  },
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fraxken/size-satisfies/issues"
  },
  "homepage": "https://github.com/fraxken/size-satisfies#readme",
  "dependencies": {
    "bytes": "^3.1.0"
  },
  "devDependencies": {
    "@slimio/eslint-config": "^4.1.0",
    "@types/node": "^14.10.1",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2"
  }
}
